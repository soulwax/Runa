{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug (Build and Run)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/debug_new/Runa2${command:cmake.launchTargetFileExtension}",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "windows": {
        "program": "${workspaceFolder}/build/debug_new/Runa2.exe",
        "MIMode": "gdb",
        "miDebuggerPath": "gdb.exe"
      },
      "linux": {
        "program": "${workspaceFolder}/build/debug_new/Runa2",
        "MIMode": "gdb",
        "miDebuggerPath": "gdb"
      },
      "osx": {
        "program": "${workspaceFolder}/build/debug_new/Runa2",
        "MIMode": "lldb"
      },
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build (Debug)",
      "logging": {
        "moduleLoad": false,
        "trace": false,
        "engineLogging": false,
        "programOutput": true,
        "exceptions": true
      }
    },
